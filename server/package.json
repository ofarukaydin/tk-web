{
  "name": "typescript-location-weather-example",
  "version": "0.3.2",
  "license": "MIT",
  "private": true,
  "scripts": {
    "predev": "yarn mesh:ts",
    "dev": "ts-node-dev src/index.ts",
    "prestart": "yarn mesh:ts",
    "start": "ts-node src/index.ts",
    "premesh:serve": "yarn mesh:ts",
    "mesh:serve": "graphql-mesh serve",
    "mesh:ts": "graphql-mesh typescript --output ./src/mesh/__generated__/types.ts"
  },
  "devDependencies": {
    "@types/cors": "^2.8.7",
    "@types/express": "4.17.7",
    "@types/express-graphql": "0.9.0",
    "@types/node": "14.6.0",
    "ts-node": "8.10.2",
    "ts-node-dev": "1.0.0-pre.56",
    "typescript": "3.9.7"
  },
  "dependencies": {
    "@graphql-mesh/cli": "0.3.2",
    "@graphql-mesh/config": "0.4.1",
    "@graphql-mesh/openapi": "0.4.1",
    "@graphql-mesh/runtime": "0.3.1",
    "@graphql-mesh/transform-cache": "0.3.1",
    "apollo-server": "^2.16.1",
    "cors": "^2.8.5",
    "express": "4.17.1",
    "express-graphql": "^0.11.0",
    "graphql": "15.3.0"
  }
}
